<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import com.jfoenix.controls.JFXDrawer?>
<?import com.jfoenix.controls.JFXHamburger?>
<?import com.jfoenix.controls.JFXToolbar?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import com.jfoenix.transitions.hamburger.HamburgerBackArrowBasicTransition?>

<StackPane fx:id="root" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <BorderPane fx:id="mainBorderPane">
    <top>
      <VBox fx:id="mainVBoxTop" prefHeight="51.0" prefWidth="537.0" BorderPane.alignment="CENTER">
        <children>
          <JFXToolbar fx:id="mainToolbarTop" prefHeight="51.0" prefWidth="354.0">
            <leftItems>
              <JFXHamburger fx:id="mainHamburgerTop" BorderPane.alignment="CENTER">
                <HamburgerBackArrowBasicTransition />
              </JFXHamburger>
              <Label>Maxi Rename</Label>
            </leftItems>
            <padding>
              <Insets left="10.0" />
            </padding>
          </JFXToolbar>
        </children>
      </VBox>
    </top>
    <center>
      <JFXDrawer fx:id="mainDrawer" defaultDrawerSize="160" BorderPane.alignment="CENTER" />
    </center>
  </BorderPane>
</StackPane>
